{
  "name": "suavekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync && npm run smui-theme",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@smui/button": "^6.0.0-beta.16",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/form-field": "^6.0.0-beta.16",
    "@smui/paper": "^6.0.0-beta.16",
    "@smui/radio": "^6.0.0-beta.16",
    "smui-theme": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "1.0.0-next.350",
      "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "unocss": "^0.38.1"
  },
  "type": "module"
}
